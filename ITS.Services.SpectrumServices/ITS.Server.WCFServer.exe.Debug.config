<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory>
      <mapping assembly="ITS.Data.RoadRepairing" xdt:Transform="Insert" />
    </session-factory>
  </hibernate-configuration>
  <unity>
    <container>
      <register type="ITS.Core.ServiceInterfaces.IServiceConfigurator, ITS.Core"
                      mapTo="ITS.Services.RoadRepairingServices.RoadRepairServiceConfigurator, ITS.Services.RoadRepairingServices"
                      name="RoadRepairing" xdt:Transform="Insert" />
    </container>
  </unity>
</configuration>